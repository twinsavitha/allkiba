 create table delivered (id int not null auto_increment, name varchar(100) not null,item varchar(100) not null, primary key(id));
 create table deliver (id int not null auto_increment, name varchar(100) not null,location varchar(100) not null,item varchar(100) not null,amount int not null, primary key(id));
 create table stores(id int not null auto_increment, name varchar(100) not null,location varchar(100),  primary key(id));
 ALTER TABLE items ADD quantity VARCHAR(100) AFTER name;
 ALTER TABLE items MODIFY quantity INTEGER;